<RoutingRules>
  <RoutingRule>
    <Condition>
      <KeyPrefixEquals>feed/rss</KeyPrefixEquals>
      <HttpErrorCodeReturnedEquals>403</HttpErrorCodeReturnedEquals>
    </Condition>
    <Redirect>
      <ReplaceKeyWith>feed/rss/index.xml</ReplaceKeyWith>
    </Redirect>
  </RoutingRule>
</RoutingRules>
